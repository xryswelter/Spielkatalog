<div id="home">
    <div class="container">
        <div class="row">
            <div class="col-8 offset-2">
                <br>
                <h1>{{msg}}</h1>
                <br>
                <ul id="example-list" class="list-group">
                    {{#each examples}}
                    <li data-id="{{this.id}}" class="list-group-item">
                        <div class="media gameMedia">
                            <img class="mr-3 gamePicture img-thumbnail img-fluid" src="{{this.gamePicture}}" alt="{{this.gameName}}">
                            <div class="media-body">
                                <h5 class="mt-0 gameName" id="gameName">{{this.gameName}}</h5>
                                <p id="gameBio">{{this.summary}}</p>
                                <p class="ownedStatus">Owned status: {{this.ownedStatus}}</p>
                                <a href="{{giantbombURL}}" target="_blank">Link to Giant Bomb</a>

                            </div>
                        </div>
                        <button class="btn btn-danger float-right editButton" data-id="{{this.id}}">Edit</button>
                    </li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>